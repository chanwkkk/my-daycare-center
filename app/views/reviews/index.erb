<h1>All reviews: </h1>
<ol>

  <% @reviews.each do |r|%>

    <li>User <%= r.user.username  %> rates Center <a href="/centers/<%= r.center.id%>"> <%= r.center.name %></a> <%= r.rate.rate_value%>
    <a href="/reviews/<%= r.id %>">Read content</a>
    <br>
  <% end %>
</ol>

<h3><a href="/reviews/new">Create your own review</a></h3>
