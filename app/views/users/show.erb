<h1><%= @user.username %></h1>

<h2>Your reviews:</h2>
<ol>
  <% @reviews.each do |r|%>
    <p><li><%= r.user.username  %> rates Center <a href="/centers/<%= r.center.id%>"> <%= r.center.name %></a> <%= r.rate%>: <%= r.content%></li></p>
    <br>
  <% end %>
</ol>
